         <font>
            <Font name="Franklin Gothic Medium" size="17.0" />
         </font>
      </Button>
      <Button fx:id="pauseButton" layoutX="611.0" layoutY="175.0" mnemonicParsing="false" onAction="#pauseMedia" text="PAUSE" textFill="#830707">
         <font>
            <Font name="Franklin Gothic Medium" size="17.0" />
         </font>
      </Button>
      <Button fx:id="resetButton" layoutX="611.0" layoutY="245.0" mnemonicParsing="false" onAction="#resetMedia" text="RESET" textFill="#830707">
         <font>
            <Font name="Franklin Gothic Medium" size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
